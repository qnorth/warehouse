{
    "name": "qnorth/warehouse",
    "type": "project",
    "description": "warehouse manager",
    "authors": [
        {
            "name": "qnorth",
            "email": "north.webdev@gmail.com"
        }
    ],
    "require": {
        "php": "7.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "qossmic/deptrac-shim": "^0.11.1"
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "deptrac": "vendor/bin/deptrac analyze --formatter=graphviz --graphviz-dump-image=\"./dep.png\""
    }
}
